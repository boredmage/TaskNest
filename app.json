{
  "expo": {
    "scheme": "tasknest",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "name": "TaskNest",
    "slug": "tasknest",
    "icon": "./assets/icon.png",
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/inter/400Regular/Inter_400Regular.ttf",
            "node_modules/@expo-google-fonts/inter/500Medium/Inter_500Medium.ttf",
            "node_modules/@expo-google-fonts/inter/600SemiBold/Inter_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/inter/700Bold/Inter_700Bold.ttf",
            "node_modules/@expo-google-fonts/inter/800ExtraBold/Inter_800ExtraBold.ttf",
            "node_modules/@expo-google-fonts/inter/900Black/Inter_900Black.ttf"
          ]
        }
      ],
      "expo-localization",
      "expo-secure-store"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.qudusayo.tasknest",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.qudusayo.tasknest"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "df385772-ead4-441c-91d2-339a03084c37"
      }
    },
    "owner": "qudusayo"
  }
}
